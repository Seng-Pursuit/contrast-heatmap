{
  "productName": "Contrast Heatmap",
  "version": "0.1.0",
  "identifier": "com.contrastheatmap.desktop",
  "build": {
    "beforeDevCommand": "node ../ui/build.mjs",
    "beforeBuildCommand": "node ../ui/build.mjs",
    "devUrl": null,
    "frontendDist": "../ui-dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Contrast Heatmap",
        "width": 900,
        "height": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": ["icons/icon.icns", "icons/icon.png"]
  }
}


